<Window x:Class="SyglHost.EidtPopTime"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="修改弹出时间" Height="200" Width="300">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <WrapPanel  HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label>弹出时间：</Label>
            <TextBox Name="PopTimeTextBox" Width="80"></TextBox>
        </WrapPanel>
        <WrapPanel Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Right">
        <Button Height="25" Width="60"  Margin="10,0" Name="CancelEditBtn" Click="CancelEditBtn_Click_1">取消</Button>
        <Button Height="25" Width="60"  Margin="10,0" Name="SubmitEditBtn" Click="SubmitEditBtn_Click_1">确定</Button>
        </WrapPanel>
    </Grid>
</Window>
